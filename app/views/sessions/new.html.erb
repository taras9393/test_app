<h1>Login</h1>
<% provide(:ttl, "Login") %>

<div class = "row">
 <div class = "col-md-6 col-md-offset-3">

    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :enter_email %>
      <%= f.email_field :eemail, class: "form-control" %>

      <%= f.label "Enter password" %>
      <%= f.password_field :ppassword, class: "form-control" %>

      <%= f.submit "log in", class: "btn btn-primary" %>
    <% end %>
     <p>"New user? <%= link_to "Create account now!!", signup_path %>"</p>
 </div>
</div>
